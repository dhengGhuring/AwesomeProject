{
    "root": true,
    "extends": ["@react-native", "prettier"],
    "rules": {
        "react-native/no-inline-styles": 0,
        // "no-unused-vars": "off",
        "no-unused-vars": [
            "error",
            {"vars": "all", "args": "after-used", "ignoreRestSiblings": false}
        ],
        "quotes": ["error", "double", {"allowTemplateLiterals": true}],
        "prettier/prettier": ["error"]
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
